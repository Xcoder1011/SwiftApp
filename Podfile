  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  
  pod 'RxSwift', '6.6.0'  #https://github.com/ReactiveX/RxSwift
  pod 'RxCocoa', '6.6.0'

  # Tools
